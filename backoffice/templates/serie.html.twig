{% extends "layout.html.twig" %}

{% block body %}

<!-- Page Content -->
    <div class="container">

      <!-- Jumbotron Header -->
      <header class="jumbotron my-4">
        <h1 class="display-3">Serie de {{data.ville}}</h1>
      </header>

      <!-- Page Features -->
      <div class="row text-center">

	    <div class="col-lg-3 col-md-6 mb-4">
	      <div class="card">
	        <div class="card-body">
            <h4 class="card-title"><a class="nav-link" href="{{ path_for('serie', { 'id': data.id }) }}">{{ data.ville }}</a></h4>
            <hr>
	          <p class="card-text">{{ data.longitude }}</p>
	          <p class="card-text">{{ data.latitude }}</p>
	        </div>
	      </div>
	    </div>



      </div>
      <!-- /.row -->

    </div>
    <!-- /.container -->

{% endblock %}